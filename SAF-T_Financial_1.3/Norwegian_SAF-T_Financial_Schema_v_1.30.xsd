	<xs:complexType name="PartyInfoStructure">
		<xs:annotation>
			<xs:documentation>Additional party information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PaymentTerms" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Payment terms of the party.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Days" type="xs:nonNegativeInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Days of respite before due date from invoice date.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Months" type="xs:nonNegativeInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Months of respite before due date from invoice date.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CashDiscountDays" type="xs:nonNegativeInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Number of days from the invoice date the cash discount can be deducted.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CashDiscountRate" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Rate for calculating cash discount.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:minInclusive value="0.00"/>
									<xs:maxInclusive value="100.00"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="FreeBillingMonth" type="xs:boolean" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Indicator that states whether free billing month is used or not. Free billing month sets the deadline to the last date of the invoice month.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NaceCode" type="SAFshorttextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>NACE (Nomenclature of Economic Activities) is the European statistical classification of economic activities.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencyCode" type="ISOCurrencyCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Three-letter currency code according to ISO 4217 standard. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of party.
Enumerated: Private, Company, Government</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="SAFmiddle1textType">
						<xs:enumeration value="Private"/>
						<xs:enumeration value="Company"/>
						<xs:enumeration value="Government"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Status" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Type of account.
Enumerated: Active, Observation, Passive.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="SAFmiddle1textType">
						<xs:enumeration value="Active"/>
						<xs:enumeration value="Observation"/>
						<xs:enumeration value="Passive"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Analysis" type="AnalysisPartyInfoStructure" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Standard analysis codes for the party, such as project, department, cost center, groups, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Notes" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Notes.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
